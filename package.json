{
  "name": "puzzled",
  "version": "1.0.0",
  "description": "React interactive puzzle app.",
  "main": "server/server.js",
  "scripts": {
    "start": "node server --ignore client",
    "develop": "concurrently \"cd server && npm run start\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrhappywaves/puzzled.git"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^5.1.0"
  },
  "bugs": {
    "url": "https://github.com/mrhappywaves/puzzled/issues"
  },
  "homepage": "https://github.com/mrhappywaves/puzzled#readme"
}
